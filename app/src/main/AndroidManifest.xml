<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teddy.abahmartabak">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcherxhdi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcherxhdi"
        android:supportsRtl="true"
        android:theme="@style/Theme.AbahMartabak">
        <activity android:name=".RateActivity"></activity>
        <activity android:name=".MenuActivity" />
        <activity android:name=".KataMerekaActivity" />
        <activity android:name=".OwnerActivity" />
        <activity android:name=".KontakActivity" />
        <activity android:name=".AlamatActivity" />
        <activity android:name=".PromoActivity" />
        <activity android:name=".TentangActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>